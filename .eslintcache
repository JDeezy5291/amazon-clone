t [{basket, user}, dispatch] = useStateValue()\r\n    const [showMenu, setShowMenu] = useState(false)\r\n    const handleAuthentication = () => {\r\n        if (user) {\r\n            auth.signOut()\r\n        }\r\n    }\r\n    \r\n    const [isMobile, setIsMobile] = useState(window.innerWidth < 930);\r\n\r\n    useEffect(() => {\r\n        window.addEventListener(\"resize\", () => {\r\n            const ismobile = window.innerWidth < 930;\r\n            if (ismobile !== isMobile) setIsMobile(ismobile);\r\n        }, false);\r\n    }, [isMobile]);\r\n\r\n\r\n    return (\r\n        <div className=\"header\">\r\n            <Link to=\"/\">\r\n                <img className=\"header__logo\" \r\n                    src=\"http://pngimg.com/uploads/amazon/amazon_PNG11.png\" \r\n                    alt=\"amazon logo\"\r\n                />\r\n            </Link>\r\n            <div\r\n                className=\"header__address\">\r\n                <PlaceIcon className=\"header__optionPlace\" />\r\n                <div className=\"header__addressOptions\">\r\n                    <span className=\"header__optionLineOne\">\r\n                        {user ? 'Deliver to ' + user?.email : 'Hello, guest'}\r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        {user ? 'Sometown 18204' : 'Select your address'}\r\n                    </span>\r\n                </div>\r\n            </div>\r\n            <div className=\"header__search\">\r\n                <input \r\n                    className=\"header__searchInput\"\r\n                    type=\"text\"\r\n                />\r\n                <SearchIcon className=\"header__searchIcon\" />\r\n            </div>\r\n            <MenuIcon \r\n                onClick={() => setShowMenu(!showMenu)}\r\n                className={`menuIcon`}\r\n                // ${showMenu ? \"header__navOptionsToggle\" : null} \r\n            />\r\n            {/* <div className={`header__nav ${showMenu ? \"showMenu\" : \"hideMenu\"}`}> */}\r\n            \r\n            <div className={`header__nav ${isMobile && showMenu ? \"showMenu\" : \"hideMenu\"}`}>\r\n                {/* ${showMenu ? \"showMenu\" : null} */}\r\n            {/* <div className={`header__navOptions`}> */}\r\n            <div className={`header__nav ${isMobile && showMenu ? \"header__OptionsOff\" : \"header__navOptions\"}`}>                           \r\n            <Link to={!user && \"/login\"}>\r\n                <div \r\n                    onClick={handleAuthentication}\r\n                    className=\"header__option\">\r\n                    <span className=\"header__optionLineOne\">\r\n                        Hello, {user ? user?.email : 'guest'}\r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        {user ? 'Sign Out' : 'Sign In'}\r\n                    </span>\r\n                </div>\r\n            </Link>\r\n            <Link to ='/orders'>\r\n                <div className=\"header__option\">\r\n                    <span className=\"header__optionLineOne\">\r\n                        Returns\r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        & Orders\r\n                    </span>\r\n                </div>\r\n            </Link>\r\n                <div className=\"header__option\">\r\n                    <span className=\"header__optionLineOne\">\r\n                        Your\r\n                    </span>\r\n                    <span className=\"header__optionLineTwo\">\r\n                        Prime\r\n                    </span>\r\n                </div>\r\n                <Link to=\"/checkout\">\r\n                    <div className=\"header__optionBasket\">\r\n                        <ShoppingBasketIcon />\r\n                        <span className=\"header__optionLineTwo header__basketCount\">{basket?.length}</span>\r\n                    </div>\r\n                </Link>\r\n            </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Header\r\n","C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\Product\\Product.js",["87","88"],"import React from \"react\";\r\nimport { useStateValue } from \"../../StateProvider\";\r\nimport \"./Product.css\";\r\n\r\nfunction Product({id, title, image, price, rating}) {\r\n  // state of global store, dispatch, how to manipulate the data\r\n  const [{basket}, dispatch] = useStateValue()\r\n\r\n  const addToBasket = () => {\r\n    // dispatch item into data layer\r\n    dispatch({\r\n      type: 'ADD_TO_BASKET',\r\n      item: {\r\n        id: id,\r\n        title: title,\r\n        image: image,\r\n        price: price,\r\n        rating: rating\r\n      }\r\n    })\r\n  }\r\n  return (\r\n    <div className=\"product\">\r\n      <div className=\"product__info\">\r\n        <p>{title}</p>\r\n        <p className=\"product__price\">\r\n          <small>$</small>\r\n          <strong>{price}</strong>\r\n        </p>\r\n        <div className=\"product__rating\">\r\n          {Array(rating).fill().map((_,i) => (\r\n            <p>⭐</p>\r\n            ))}\r\n        </div>\r\n      </div>\r\n      <img\r\n        src={image}\r\n      />\r\n      <button onClick={addToBasket}>Add to Cart</button>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Product\r\n","C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\Checkout\\Checkout.js",["89"],"import React from 'react'\r\nimport './Checkout.css'\r\nimport Subtotal from '../Subtotal/Subtotal'\r\nimport BasketItem from '../BasketItem/BasketItem'\r\nimport { useStateValue } from '../../StateProvider'\r\n\r\nfunction Checkout() {\r\n    const [{basket, user}, dispatch] = useStateValue()\r\n    return (\r\n        <div className=\"checkout\">\r\n            <div className=\"checkout__left\">\r\n                <img src=\"https://images-na.ssl-images-amazon.com/images/G/02/UK_CCMP/TM/OCC_Amazon1._CB423492668_.jpg\" alt=\"amazon ad\" className=\"checkout__ad\"/>\r\n                <div>\r\n                    <h3>Hello, {user?.email}</h3>\r\n                    <h2 className=\"checkout__title\">Your Shopping Cart</h2>\r\n                    {basket.map(item => (\r\n                        <BasketItem \r\n                            id={item.id}\r\n                            title={item.title}\r\n                            image={item.image}\r\n                            price={item.price}\r\n                            rating={item.rating}\r\n                        />\r\n                    ))}\r\n                </div>\r\n            </div>\r\n            \r\n            <div className=\"checkout__right\">\r\n                <Subtotal />\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Checkout\r\n","C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\Subtotal\\Subtotal.js",["90","91"],"import React from 'react'\r\nimport './Subtotal.css'\r\nimport CurrencyFormat from 'react-currency-format'\r\nimport { ShoppingBasket } from '@material-ui/icons'\r\nimport { useStateValue } from '../../StateProvider'\r\nimport { getBasketTotal } from '../../reducer'\r\nimport { useHistory } from 'react-router-dom'\r\n\r\nfunction Subtotal() {\r\n    // useHistory gets browser history\r\n    const history = useHistory()\r\n    const [{basket}, dispatch] = useStateValue()\r\n\r\n    return (\r\n        <div className=\"subtotal\">\r\n            <CurrencyFormat \r\n                renderText={(value) => (\r\n                    <>\r\n                    <p>\r\n                        Subtotal ({basket.length} items):\r\n                        <strong>{`${value}`}</strong>\r\n                    </p>\r\n                    <small className=\"subtotal__gift\">\r\n                        <input type=\"checkbox\"/>This order contains a gift\r\n                    </small>\r\n                    </>\r\n                )}\r\n                decimalScale={2}\r\n                value={getBasketTotal(basket)}\r\n                displayType={\"text\"}\r\n                thousandSeparator={true}\r\n                prefix={\"$\"}\r\n            />\r\n            <button onClick={e => history.push('/payment')}>Proceed to checkout</button>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Subtotal\r\n","C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\reducer.js",[],"C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\StateProvider.js",[],"C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\BasketItem\\BasketItem.js",["92","93"],"import React from 'react'\r\nimport { useStateValue } from '../../StateProvider'\r\nimport './BasketItem.css'\r\n\r\nfunction BasketItem({id, title, image, price, rating, hideButton}) {\r\n    const [{ basket }, dispatch] = useStateValue()\r\n    const removeFromCart = () => {\r\n        dispatch({\r\n            type: 'REMOVE_FROM_CART',\r\n            id: id  \r\n        })\r\n    }\r\n    return (\r\n        <div className=\"basketItem\">\r\n            <img src={image} className=\"basketItem__image\"/>\r\n            <div className=\"basketItem__info\">\r\n                <p className=\"basketItem__title\">{title}</p>\r\n                <p className=\"basketItem__price\">\r\n                    <small>$</small>\r\n                    <strong>{price}</strong>\r\n                </p>\r\n                <div className=\"basketItem__rating\">\r\n                    {Array(rating).fill().map((_,i) => (\r\n                        <p>⭐</p>\r\n                    ))}\r\n                </div>\r\n                {!hideButton && (\r\n                    <button onClick={removeFromCart}>Remove from Cart</button>\r\n                )}\r\n            </div> \r\n        </div>\r\n    )\r\n}\r\n\r\nexport default BasketItem\r\n","C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\Login\\Login.js",[],"C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\firebase.js",[],"C:\\Users\\Second Account\\Desktop\\future_portfolio_projects\\amazon-clone\\src\\Components\\Payment\\Payment.js",["94"],"import React, { useEffect, useState } from 'react'\r\nimport { useStateValue } from '../../StateProvider'\r\nimport BasketItem from '../BasketItem/BasketItem'\r\nimport './Payment.css'\r\nimport { Link, useHistory } from 'react-router-dom'\r\nimport { CardElement, useElements, useStripe } from '@stripe/react-stripe-js'\r\nimport CurrencyFormat from 'react-currency-format'\r\nimport { getBasketTotal } from '../../reducer'\r\nimport axios from '../../axios'\r\nimport { db } from '../../firebase'\r\n\r\nfunction Payment() {\r\n    const [{basket, user}, dispatch] = useStateValue()\r\n    const history = useHistory()\r\n\r\n    const [error, setError] = useState(null)\r\n    const [disabled, setDisabled] = useState(true)\r\n    const [succeeded, setSucceeded] = useState(false)\r\n    const [processing, setProcessing] = useState(\"\")\r\n    const [clientSecret, setClientSecret] = useState(true)\r\n\r\n    const stripe = useStripe()\r\n    const elements = useElements()\r\n\r\n    const handleSubmit = async (e) => {\r\n        // prevent refreshing\r\n        e.preventDefault()\r\n        // prevent user from clicking buy now button multiple times\r\n        setProcessing(true)\r\n\r\n        const payload = await stripe.confirmCardPayment(clientSecret, {\r\n            payment_method: {\r\n                card: elements.getElement(CardElement)\r\n            }\r\n        }).then(({paymentIntent}) => {\r\n            // paymentIntent = payment confirmation\r\n\r\n            db\r\n                .collection('users')\r\n                .doc(user?.uid)\r\n                .collection('orders')\r\n                .doc(paymentIntent.id)\r\n                .set({\r\n                    basket: basket,\r\n                    amount: paymentIntent.amount,\r\n                    created: paymentIntent.created\r\n                })\r\n\r\n            setSucceeded(true)\r\n            setError(null)\r\n            setProcessing(false)\r\n\r\n            dispatch({\r\n                type: 'EMPTY_CART'\r\n            })\r\n\r\n            history.replace('/orders')\r\n        })\r\n    }\r\n    const handleChange = e => {\r\n        // listen for changes in CardElement\r\n        setDisabled(e.empty)\r\n        setError(e.error ? e.error.message : \"\")\r\n    }\r\n\r\n    useEffect(() => {\r\n        const getClientSecret = async () => {\r\n            const response = await axios({\r\n                method: 'post',\r\n                url: `/payments/create?total=${getBasketTotal(basket) * 100}`\r\n            })\r\n            setClientSecret(response.data.clientSecret)\r\n        }\r\n        getClientSecret()\r\n    }, [basket])\r\n\r\n    // console.log('THE CLIENT SECRET >>>>', clientSecret)\r\n    \r\n    return (\r\n        <div className=\"payment\">\r\n            <div className=\"payment__container\">\r\n            <h1>Checkout (<Link to=\"/checkout\">{basket?.length} items</Link>)</h1>\r\n                <div className=\"payment__section\">\r\n                    <div className=\"payment__title\">\r\n                        <h3>Delivery Address</h3>\r\n                    </div>\r\n                    <div className=\"payment__address\">\r\n                        <p>{user?.email}</p>\r\n                        <p>209 React Lane</p>\r\n                        <p>Philadelphia, PA</p>\r\n                    </div>\r\n                </div>\r\n                <div className=\"payment__section\">\r\n                    <div className=\"payment__title\">\r\n                        <h3>Review items and delivery</h3>\r\n                    </div>\r\n                    <div className=\"payment__items\">\r\n                        {basket.map(item => (\r\n                            <BasketItem \r\n                                id={item.id}\r\n                                title={item.title}\r\n                                image={item.image}\r\n                                price={item.price}\r\n                                rating={item.rating}\r\n                            />\r\n                        ))}\r\n                    </div>\r\n                </div>\r\n                <div className=\"payment__section\">\r\n                    <div className=\"payment__title\">\r\n                        <h3>Payment Method</h3>\r\n                    </div>\r\n                    <div className=\"payment__details\">\r\n                        <form onSubmit={handleSubmit}>\r\n                            <CardElement onChange={handleChange} />\r\n                            <div className=\"payment__priceContainer\">\r\n                                <CurrencyFormat \r\n                                    renderText={(value) => (\r\n                                        <>\r\n                                        <h3>Order Total: {value}</h3>\r\n                                        </>\r\n                                    )}\r\n                                    decimalScale={2}\r\n                                    value={getBasketTotal(basket)}\r\n                                    displayType={'text'}\r\n                                    thousandSeparator={true}\r\n                                    prefix={'$'}\r\n                                />\r\n                                <button disabled={processing || disabled || succeeded}>\r\n                                    <span>{processing ? <p>Processing</p> : \"Buy Now \"}</span>\r\n                                </button>\r\n                            </div>\r\n                            {/* 